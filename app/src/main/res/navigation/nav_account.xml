<?xml version="1.0" encoding="utf-8"?>
<navigation
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/nav_account"
        app:startDestination="@id/accountFragment">

    <fragment android:id="@+id/accountFragment"
            android:name="com.smartcity.provider.ui.main.store.StoreFragment"
            android:label="@string/fragment_account"
            tools:layout="@layout/fragment_store">

        <action
                android:id="@+id/action_accountFragment_to_viewProductFragment2"
                app:destination="@id/viewProductFragment2" />
    </fragment>
    <fragment
            android:id="@+id/viewProductFragment2"
            android:name="com.smartcity.provider.ui.main.custom_category.viewProduct.ViewProductFragment"
            android:label="fragment_view_product"
            tools:layout="@layout/fragment_view_product" >
        <action
                android:id="@+id/action_viewProductFragment2_to_accountFragment"
                app:destination="@id/accountFragment" />
    </fragment>


</navigation>